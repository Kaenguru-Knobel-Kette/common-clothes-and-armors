function Process(e: IInterface): Integer;
var
  i, oldFormID, newFormID: Integer;
  sOldFormID, sNewFormID: String;
begin
  if Equals(GetFile(e), cca) then begin
    oldFormID := GetLoadOrderFormID(e);
    sOldFormID := IntToHex(oldFormID, 8);
    i := slFormIDMap.IndexOfName(sOldFormID);
    if i <> -1 then begin
      sNewFormID := slFormIDMap.ValueFromIndex[i];
      newFormID := StrToInt('$' + sNewFormID);
      while ReferencedByCount(e) > 0 do
        CompareExchangeFormID(ReferencedByIndex(e, 0), oldFormID, newFormID);
      while OverrideCount(e) > 0 do
        SetLoadOrderFormID(OverrideByIndex(e, 0), newFormID);
      SetLoadOrderFormID(e, newFormID);
    end;
  end;
end;


function Finalize: Integer;
begin
  slFormIDMap.Free;
end;

